<%= simple_form_for @list do |f| %>
  <div class="form-group">
    <%= f.input :name, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.input :photo, as: :file %>
  </div>
  <%= f.submit 'Create list', class: 'btn btn-primary' %>
  <%= link_to 'Back to lists', root_path, class: 'btn btn-danger' %>
<% end %>
